#' @rdname GMWeibull
dgmweibull<-function(x, alpha, gamma, lambda, beta, log = FALSE)
{
dens <- (alpha*beta*x^(gamma-1)*(gamma+lambda*x)*exp(lambda*x-alpha*x^gamma*exp(lambda*x)))/((1-exp(-alpha*x^gamma*exp(lambda*x)))^(1-beta))
if(log[1]) dens<-log(dens)
ret <- dens
ret
}
